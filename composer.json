    "description": "Simple AES-256-CBC encrypter decrypter",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Encrypter",
        "Cryptoghraphy"
        
    ],
    
    "autoload": {
        "psr-4": {
            "Fixer112\\Encrypter\\": "src/",
            "Tests\\": "tests/"
        }
    },
