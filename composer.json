            "name": "fixer112",
            "email": "25584514+fixer112@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.1",
       "illuminate/filesystem": "^12.0",
        "illuminate/support": "^12.0",
        "phpoption/phpoption": "^1.9",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "pestphp/pest": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
